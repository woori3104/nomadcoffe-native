{
  "expo": {
    "name": "instaclone-native",
    "slug": "instaclone-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.wooriNomadCoffee.nomadcoffee",
      "buildNumber": "1.0.0"
      },
    "android": {
      "package": "com.wooriNomadCoffee.nomadcoffee",
      "versionCode": 1
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
